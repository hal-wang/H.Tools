  </ItemGroup>

  <ItemGroup Condition="'$(TargetFramework)' == 'net452'">
    <Compile Remove="Uwp\**" />
    <EmbeddedResource Remove="Uwp\**" />
    <None Remove="Uwp\**" />
  </ItemGroup>
  <ItemGroup Condition="'$(TargetFramework)' == 'netstandard2.0'">
    <Compile Remove="Uwp\**" />
