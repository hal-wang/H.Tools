    <LangVersion>9.0</LangVersion>
    <Authors>h.wang</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Company></Company>
    <PackageIcon>logo.png</PackageIcon>
    <PackageProjectUrl>https://github.com/hbrwang/Tools</PackageProjectUrl>
    <RepositoryUrl>https://github.com/hbrwang/Tools</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Version>1.1.0</Version>
    <Description>工具库</Description>
    <AssemblyVersion>1.1.0.0</AssemblyVersion>
    <FileVersion>1.1.0.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="LICENSE" />
    <None Remove="Uwp\Controls\Color\ColorSelecter.xaml" />
    <None Remove="Uwp\Controls\Dialog\DialogStyle.xaml" />
    <None Remove="Uwp\Controls\Emoji\EmojiSelecter.xaml" />
    <None Remove="Uwp\Controls\Font\FontSelecter.xaml" />
    <None Remove="Uwp\Controls\Loading\LayoutLoading.xaml" />
    <None Remove="Uwp\Controls\Loading\PartialLoading.xaml" />
    <None Remove="Uwp\Controls\Message\MessageStyle.xaml" />
    <None Remove="Uwp\Controls\Message\NotifyPopup.xaml" />
